		<hotfixPath path="net/request/func/base" needFactory="false"/>
		<hotfixPath path="net/request/scene/base" needFactory="false"/>
		<hotfixPath path="net/response/base" needFactory="false"/>
		<hotfixPath path="net/response/func/base" needFactory="false"/>
		<hotfixPath path="net/response/scene/base" needFactory="false"/>

		<ignorePath path="adapters"/>
		<ignorePath path="control/CILRuntimeControl.cs"/>
		<ignorePath path="tool/generate"/>
		<ignorePath path="tool/func"/>

		<ignorePath path="logic/unit/AttributeDataLogic.cs"/>
