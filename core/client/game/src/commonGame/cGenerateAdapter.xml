		<hotfixPath path="logic" needFactory="true"/>
		<hotfixPath path="view" needFactory="true"/>

		<hotfixPath path="config/game" needFactory="false"/>
		<hotfixPath path="part" needFactory="false"/>

		<hotfixPath path="net/base" needFactory="false"/>
		<hotfixPath path="net/centerResponse/func/base" needFactory="false"/>
